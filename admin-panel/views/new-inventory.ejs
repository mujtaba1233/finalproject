<div class="modal fade" id="modalID" role="dialog"  tabindex="-1" aria-labelledby="myModalLabel" aria-hidden="true">
	<div class="modal-dialog modal-lg">
		<!-- Modal content-->
		<div class="modal-content">
			<div class="panel panel-default">
				<div class="modal-header panel-heading  d-flex justify-content-between">
					<h3>Add Serials</h3>
					<button type="button" class="close" ng-click="close(undefined)">&times;</button>
					</h4>
				</div>
			</div>
			<div class="modal-body" style="height: auto; overflow-y: auto; overflow-x: hidden;">
				<!-- <form id="serialForm" ng-submit="addInventory()"> -->
					<div class="row">
						<!-- <div class="col-md-6 form-group" >
							<input ng-required="!inventory.stockType" id="ship" type="radio" value="shipping" ng-model="inventory.stockType" class="form-group">
							<label class="form-group" for="ship"> Shipping Stock</label>
						</div>
						<div class="col-md-6 form-group" >
							<input ng-required="!inventory.stockType" id="prod" type="radio" value="production" ng-model="inventory.stockType" class="form-group">
							<label class="form-group" for="prod"> Pre-Production Stock</label>
						</div> -->
						<div class="col-md-6 form-group" >
							<label class="form-group"> Product Code</label>
							<input ng-disabled="true" type="text" ng-model="inventory.ProductCode" class="form-control">
						</div>
						<div class="col-md-6 form-group" >
							<label class="form-group"> Serial No</label>
							<input required type="text"  ng-model="inventory.SerialNO" id="SerialNumber" ng-change="barCodeScan()" ng-model-options="{debounce: 300}" class="form-control" autofocus>
						</div>
						<div class="col-md-6 form-group" >							
						</div>
						<div class="col-md-6 form-group row" >
							<div class="col-sm-3">
								<input  ng-disabled="true" type="text" ng-model="totalProducts" class="form-control ">

							</div>
							<input type="button" style="margin-right: 5px;" value="Remove Last" name="Remove Last" title="Remove Last" ng-click="lastBarcodeValueRemove()" class="btn btn-primary  col-sm-4">
							<input type="button" style="margin-right: 5px;" value="Clear All" name="Clear_All" title="Clear All" ng-click="removeBarCodeAll()" class="btn btn-primary  col-sm-3">
							
						</div>
					</div>
					
				<!-- </form> -->
				<!-- Modal -->
			</div>
			<div class="modal-footer">
				<div class="row">
					<div class="col-md-12 form-group" >
						<input type="submit" title="Add Serial Number" value="Add"  ng-click="addInventory()" class="btn btn-success pull-right">
						<!-- <button style="margin-right: 5px;" title="Cancel" ng-click="close(undefined)" class="btn btn-primary pull-right"> Close</button> -->
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
