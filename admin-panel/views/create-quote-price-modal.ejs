<div class="modal fade" role="dialog">
	<div class="modal-dialog modal-lg">
		<!-- Modal content-->
		<div class="modal-content">
			<div class="panel panel-default">
				<div class="modal-header panel-heading  d-flex justify-content-between">
					<h3>Price Alert</h3>
					<button type="button" class="close" ng-click="close(undefined)" class="close" data-dismiss="modal" aria-label="Close">&times;</button>
					</h4>
				</div>
			</div>
			<div class="modal-body" style="height: auto; overflow-y: auto; overflow-x: hidden;">
					<div class="row">
						<div class="col-md-12 form-group" >
							<h5 class="form-group">There is a change in the price of product(s).</h5>
							<!-- <input  type="text" ng-model="reportName" class="form-control"> -->
						</div>
						
					</div>
			</div>
			<div class="modal-footer">
				<div class="row">
                    <div class="col-md-6 form-group" >
                        <input type="submit" title="PreviousPrice" value="Use Previous Price"  ng-click="updatePrice(false)" class="btn btn-success pull-left">
                   </div>
					<div class="col-md-6 form-group" >
						 <input type="submit" title="UpdatePrice" value="Update Price"    ng-click="updatePrice(true)" class="btn btn-success pull-right">
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
